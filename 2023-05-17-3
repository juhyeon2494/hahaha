public class kjhkjh {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("몇단?");
		int 몇단 = sc.nextInt();
		System.out.println("몇곱?");
		int 몇곱 = sc.nextInt();
		for(int 아이=2;아이<=몇단;아이++) {
			for(int 제이=1;제이<=몇곱;제이++) {
				System.out.println(아이+"*"+제이+"="+아이*제이);
			}
			System.out.println();
		}
	}
}
