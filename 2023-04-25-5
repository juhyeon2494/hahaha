public class kjh {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("몇단 볼래?");
		int 몇단 = sc.nextInt();
		
		System.out.println("몇까지 곱할래?");
		int 곱할수 = sc.nextInt();
		

		for (int j=2; j<=몇단; j++) {
			for (int i=1; i<= 곱할수; i++) 
				System.out.println(j+"*"+i+"="+(j*i));
			
		}
	}
}
