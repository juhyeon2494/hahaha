public class kjh {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		int evensum = 0;
		int oddsum = 0;
		for(int i=1; i<=num; i++) {
			if(i%2==0) {
				evensum += i ;
			}else if(i%2==1){
				oddsum += i;
			}
		}
		System.out.println("짝수의 합은?:"+evensum);
		System.out.println("홀수의 합은?:"+oddsum);
	}

}
