	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("몇단 볼래?");
		int 몇단 = sc.nextInt();
		
		System.out.println("몇까지 곱할래?");
		int 곱할수 = sc.nextInt();
		
		int i = 1;
		while (i<=곱할수) {
			System.out.println(몇단+"*"+i+"="+(몇단*i));
			i++;
		}
	}
}
//구구단
